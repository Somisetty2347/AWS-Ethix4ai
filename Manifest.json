{
  "name": "GENESIS",
  "description": "Your confidential information detector",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
          "activeTab",
          "tabs",
          "scripting",
          "notifications"
      ],"background": {
          "service_worker": "background.js"
      },"content_scripts": [
          {
              "matches": ["https://chatgpt.com/*"],
              "run_at": "document_end",
              "js": ["content.js"]
          }
      ],
      "host_permissions": ["https://*/"],
      "action": {
          "default_title": "Genesis",
          "default_icon": "icon.png",
          "default_popup": "popup.html"
      },
      "icons": {
        "48": "icon.png"
      }
  }
  